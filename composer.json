{
    "name": "sspat/profiru",
    "description": "Profi.ru partership program API connector",
    "type": "library",
    "version": "1.0.2",
    "keywords": [
        "profi.ru",
        "profi api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Patrik Foldes",
            "email": "studio22@mail.ru"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "2.8.1"
    },
    "autoload": {
        "psr-4": {
            "sspat\\ProfiRu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sspat\\ProfiRu\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 1800,
        "preferred-install": "dist"
    }
}
