{
    "name": "sspat/profiru",
    "description": "Profi.ru partership program API client",
    "type": "library",
    "keywords": [
        "profi.ru",
        "profi api",
        "profi.ru api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Patrik Foldes",
            "email": "studio22@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-json": "*",
        "guzzlehttp/psr7": "^1.5",
        "psr/http-client": "^1.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^5.0",
        "phpstan/phpstan": "^0.10.6",
        "phpunit/phpunit": "~5.7",
        "roave/security-advisories": "dev-master"
    },
    "suggest": {
        "php-http/guzzle6-adapter": "PSR-18 adapter for Guzzle 6"
    },
    "autoload": {
        "psr-4": {
            "sspat\\ProfiRu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sspat\\ProfiRu\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
